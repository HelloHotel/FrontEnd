Number of rooms\"\r\n                          :items=\"rooms\"\r\n                          item-text=\"name\"\r\n                ></v-select>\r\n                <v-menu\r\n                    ref=\"menu_in\"\r\n                    v-model=\"menu_in\"\r\n                    :close-on-content-click=\"false\"\r\n                    :return-value.sync=\"date_in\"\r\n                    transition=\"scale-transition\"\r\n                    offset-y\r\n                    min-width=\"290px\"\r\n                >\r\n                  <template v-slot:activator=\"{ on }\">\r\n                    <v-text-field\r\n                        v-model=\"date_in\"\r\n                        label=\"Date in\"\r\n                        prepend-icon=\"mdi-calendar\"\r\n                        readonly\r\n                        v-on=\"on\"\r\n                    ></v-text-field>\r\n                  </template>\r\n                  <v-date-picker v-model=\"date_in\" no-title scrollable>\r\n                    <div class=\"flex-grow-1\"></div>\r\n                    <v-btn text color=\"primary\" @click=\"menu_in = false\">Cancel</v-btn>\r\n                    <v-btn text color=\"primary\" @click=\"$refs.menu_in.save(date_in)\">OK</v-btn>\r\n                  </v-date-picker>\r\n                </v-menu>\r\n                <v-menu\r\n                    ref=\"menu_out\"\r\n                    v-model=\"menu_out\"\r\n                    :close-on-content-click=\"false\"\r\n                    :return-value.sync=\"date_out\"\r\n                    transition=\"scale-transition\"\r\n                    offset-y\r\n                    min-width=\"290px\"\r\n                >\r\n                  <template v-slot:activator=\"{ on }\">\r\n                    <v-text-field\r\n                        v-model=\"date_out\"\r\n                        label=\"Date out\"\r\n                        prepend-icon=\"mdi-calendar\"\r\n                        readonly\r\n                        v-on=\"